                  class=3Dnodeco=20
                  onmouseover=3D"Tip('Click the link to make this =
tooltip sticky. To close the tooltip when it\'s fixated, click =
somewhere.', TITLE, 'CLICKSTICKY &amp; CLICKCLOSE &amp; CLOSEBTN', =
CLICKSTICKY, true, CLICKCLOSE, true, CLOSEBTN, true, WIDTH, -1)"=20
                  onmouseout=3DUnTip()=20
                  =
